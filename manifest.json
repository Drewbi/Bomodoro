{
    "name": "Bomb Extension",
    "manifest_version": 2,
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "styles.css"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popUp.html",
        "default_title": "bombard"
    }
}